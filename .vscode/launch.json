    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/manage.py"
        },
        {
            "name": "Python Debugger: Django SSL",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver_plus",
                "--cert-file",
                "${workspaceFolder}/cert.pem",
                "--key-file",
                "${workspaceFolder}/key.pem"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}/manage.py"
        }

    ]
}